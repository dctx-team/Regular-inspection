# ==================== 账号配置 ====================

# 方式一：分平台配置（推荐，向后兼容）
# AnyRouter 账号配置
# ANYROUTER_ACCOUNTS='[{"name":"主账号","cookies":{"session":"xxx"},"api_user":"12345"}]'
ANYROUTER_ACCOUNTS=

# AgentRouter 账号配置
# AGENTROUTER_ACCOUNTS='[{"name":"主账号","cookies":{"session":"xxx"},"api_user":"12345"}]'
AGENTROUTER_ACCOUNTS=

# 方式二：统一配置（支持多平台和多种认证方式）
# ACCOUNTS='[
#   {
#     "name": "我的AnyRouter账号",
#     "provider": "anyrouter",
#     "cookies": {"session": "xxx"},
#     "api_user": "12345"
#   },
#   {
#     "name": "邮箱登录",
#     "provider": "anyrouter",
#     "email": {
#       "username": "your@email.com",
#       "password": "yourpassword"
#     }
#   },
#   {
#     "name": "我的AgentRouter账号",
#     "provider": "agentrouter",
#     "github": {
#       "username": "myuser",
#       "password": "mypass"
#     }
#   },
#   {
#     "name": "Linux.do账号",
#     "provider": "agentrouter",
#     "linux.do": {
#       "username": "user",
#       "password": "pass"
#     }
#   }
# ]'
ACCOUNTS=

# 自定义 Provider 配置（可选）
# PROVIDERS='{
#   "custom": {
#     "name": "自定义平台",
#     "base_url": "https://custom.example.com",
#     "login_url": "https://custom.example.com/login",
#     "checkin_url": "https://custom.example.com/api/user/checkin",
#     "user_info_url": "https://custom.example.com/api/user/self"
#   }
# }'
PROVIDERS=

# ==================== 通知配置（可选） ====================

# 邮件通知
EMAIL_USER=
EMAIL_PASS=
EMAIL_TO=
# 自定义SMTP服务器（可选）
CUSTOM_SMTP_SERVER=

# 钉钉机器人
DINGDING_WEBHOOK=

# 飞书机器人
FEISHU_WEBHOOK=

# 企业微信机器人
WEIXIN_WEBHOOK=

# PushPlus 推送
PUSHPLUS_TOKEN=

# Server酱
SERVERPUSHKEY=

# ==================== 代理配置（可选） ====================

# 全局启用代理
# USE_PROXY=true

# 方式一：订阅代理（推荐）
# PROXY_SUBSCRIPTION_URL=https://your-subscription-url
# PROXY_SELECTION_MODE=auto  # auto/manual/random
# PROXY_NODE_NAME=香港|HK|台湾|TW  # manual 模式使用
# PROXY_TEST_SPEED=true
# PROXY_CACHE_DURATION=3600

# 方式二：直接代理
# PROXY_SERVER=http://127.0.0.1:7890
# PROXY_USER=your_username
# PROXY_PASS=your_password

# 细粒度控制
# PROXY_METHODS=github,linux.do  # 仅为指定认证方式启用代理
# NO_PROXY_METHODS=cookies,email  # 为指定认证方式禁用代理

# ==================== 其他配置（可选） ====================

# CI 环境禁用的认证方式
# CI_DISABLED_AUTH_METHODS=github,linux.do

# 会话缓存加密密钥（建议设置）
# SESSION_CACHE_KEY=your_fernet_key
# 生成密钥：python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"

