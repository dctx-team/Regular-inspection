#     "email": {
#       "username": "your@email.com",
#       "password": "yourpassword"
#     }
#   },
#   {
#     "name": "我的AgentRouter账号",
#     "provider": "agentrouter",
#     "github": {
#       "username": "myuser",
#       "password": "mypass"
#     }
#   },
#   {
#     "name": "Linux.do账号",
#     "provider": "agentrouter",
#     "linux.do": {
#       "username": "user",
#       "password": "pass"
